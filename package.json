{
	"name": "orsjo-website",
	"version": "0.2.0",
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"dev": "NEXT_PUBLIC_DATOCMS_ENVIRONMENT=dev next dev --turbo & npm run codegen",
		"dev:email": "email dev",
		"build": "npm run prebuild && next build --turbo",
		"start": "next start",
		"preview": "npm run build && npm run start",
		"lint": "next lint",
		"prebuild": "tsx prebuild",
		"reset": "rm -rf ./node_modules && rm -rf pnpm-lock.yaml && rm -rf .next && pnpm i",
		"codegen": "chokidar \"./graphql/**/*.gql\" -c \"pnpm codegen:website\" & chokidar \"./lib/geins/graphql/**/*.gql\" -c \"pnpm codegen:geins\"",
		"codegen:website": "graphql-codegen --config graphql.config.ts --project default --verbose",
		"codegen:geins": "graphql-codegen --config graphql.config.ts --project geins --verbose",
		"codegen:datocms": "datocms schema:generate ./types/datocms-cma.ts",
		"shopify:sync": "tsx lib/shopify/scripts/sync-item.ts",
		"shopify:sync-all": "tsx lib/shopify/scripts/sync-all.ts"
	},
	"dependencies": {
		"@datocms/cma-client": "^5.1.29",
		"@datocms/cma-client-browser": "^5.1.29",
		"@datocms/cma-client-node": "^5.1.29",
		"@geins/core": "^0.5.0",
		"@geins/oms": "^0.5.0",
		"@geins/sdk-api-mgmt-typescript": "^2.0.1",
		"@react-email/components": "0.0.38",
		"@react-email/preview-server": "^5.0.5",
		"@sentry/nextjs": "^10.19.0",
		"@svgr/webpack": "^8.1.0",
		"@types/react": "^19.2.0",
		"chalk": "^5.6.2",
		"classnames": "^2.5.1",
		"cookies-next": "^5.1.0",
		"date-fns": "^4.1.0",
		"datocms-structured-text-utils": "^5.1.6",
		"dotenv": "^17.2.3",
		"graphql": "^16.12.0",
		"hamburger-react": "^2.5.2",
		"isbot": "^5.1.32",
		"next": "^15.5.9",
		"next-dato-utils": "github:bebejane/next-dato-utils#v2.0.85",
		"next-intl": "^4.5.5",
		"nextjs-google-analytics": "^2.3.7",
		"postmark": "^4.0.5",
		"react": "^19.2.3",
		"react-animate-height": "^3.2.3",
		"react-aria-components": "^1.10.0",
		"react-datocms": "^8.0.0-1",
		"react-dom": "^19.2.3",
		"react-hook-form": "^7.56.2",
		"react-icons": "^5.5.0",
		"react-intersection-observer": "^9.16.0",
		"react-use": "^17.6.0",
		"react-wrap-balancer": "^1.1.1",
		"rooks": "^8.0.1",
		"sass": "^1.87.0",
		"shopify-api-node": "^3.15.0",
		"swiper": "^11.2.8",
		"thenby": "^1.3.4",
		"usehooks-ts": "^3.1.1",
		"zod": "^3.22.4",
		"zustand": "^5.0.4"
	},
	"devDependencies": {
		"@geins/types": "^0.5.0",
		"@graphql-codegen/cli": "^6.1.1",
		"@graphql-codegen/typed-document-node": "^6.0.2",
		"@graphql-codegen/typescript": "^5.0.7",
		"@graphql-codegen/typescript-document-nodes": "^5.0.2",
		"@graphql-codegen/typescript-graphql-files-modules": "^3.0.1",
		"@graphql-codegen/typescript-operations": "^5.0.2",
		"@parcel/watcher": "^2.5.1",
		"@types/node": "^22.15.3",
		"@types/react-dom": "^19.2.0",
		"chokidar-cli": "^3.0.0",
		"eslint": "9.26.0",
		"eslint-config-next": "15.3.1",
		"puppeteer": "^24.29.0",
		"react-email": "^5.0.5",
		"tsx": "^4.20.6",
		"xlsx-stream-reader": "^1.1.1"
	}
}
